template <class _Ret>
struct _Vbase_compare_three_way {
     constexpr _Ret operator()(const _Vbase _Left, const _Vbase _Right) const noexcept {
        const _Vbase _Differing_bits = _Left ^ _Right;

        if (_Differing_bits == 0) {              return strong_ordering::equal;
