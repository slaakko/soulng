        auto _UOld_first       = _UFirst;
        for (_Iter_diff_t<_FwdIt> _Inc = 0; _Count_diff <= _ULast - _UOld_first;) {              _UFirst = _UOld_first + _Inc;
            if (_Pred(*_UFirst, _Val)) {                  _Iter_diff_t<_FwdIt> _Count1 = _Count_diff;
