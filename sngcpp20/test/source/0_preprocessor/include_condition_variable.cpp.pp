 constexpr _Ty* assume_aligned(_Ty* const _Ptr) noexcept   {
    if (::std:: is_constant_evaluated()) {
        return _Ptr;
    } else {
                 return static_cast<_Ty*>(__builtin_assume_aligned(_Ptr, _Nx));
    }
}

