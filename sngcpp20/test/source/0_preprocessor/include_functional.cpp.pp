            const size_type _Bucket = bucket(_Traits::_Kfn(_Eraser._Next->_Myval));                           _Nodeptr& _Bucket_lo   = _Bucket_bounds[_Bucket << 1]._Ptr;
            _Nodeptr& _Bucket_hi   = _Bucket_bounds[(_Bucket << 1) + 1]._Ptr;
            const bool _Update_lo  = _Bucket_lo == _Eraser._Next;
            const _Nodeptr _Old_hi = _Bucket_hi;
            for (;;) {                  const bool _At_bucket_back = _Eraser._Next == _Old_hi;
